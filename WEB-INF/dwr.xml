<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE dwr PUBLIC "-//GetAhead Limited//DTD Direct Web Remoting 2.0//EN" "http://www.getahead.ltd.uk/dwr//dwr20.dtd">

<dwr>
	<allow>
		<create creator="ibo" javascript="ThemesEngine" scope="application">
      		<param name="class" value="com.idega.content.themes.business.ThemesEngine" />
    	</create>
    	<convert converter="bean" match="com.idega.content.themes.helpers.bean.SimplifiedTheme" />
    	<convert converter="bean" match="com.idega.content.themes.helpers.bean.ThemeChange">
    		<param name="exclude" value="limitedSelection" />
    	</convert>
    	<convert converter="bean" match="com.idega.content.themes.helpers.bean.TreeNodeStructure" />
    	
    	<create creator="jsf" javascript="CategoriesEngine">
	    	<param name="managedBeanName" value="content.CategoriesEngineBean" />
	    	<param name="class" value="com.idega.content.business.categories.CategoriesEngineBean" />
    	</create>
    	
    	<create creator="jsf" javascript="FileUploader">
    		<param name="managedBeanName" value="fileUploader"></param>
    		<param name="class" value="com.idega.content.upload.business.FileUploaderBean"/>
		</create>
		
		<create creator="jsf" javascript="FileUploadListener">
    		<param name="managedBeanName" value="fileUploadProgressListener"></param>
    		<param name="class" value="com.idega.content.upload.business.FileUploadProgressListenerBean"/>
		</create>
	</allow>
  
	<signatures>
		<![CDATA[
  			import java.util.List;
  			import com.idega.content.themes.helpers.bean.SimplifiedTheme;
  			import com.idega.content.themes.helpers.bean.ThemeChange;
			import com.idega.content.themes.helpers.bean.TreeNodeStructure;
			
  			ThemesEngine.applyMultipleChangesToTheme(String, List<ThemeChange>, String);
  			ThemesEngine.createPage(List<TreeNodeStructure>, Boolean, String, List<String>);
  		]]>
	</signatures>
</dwr>